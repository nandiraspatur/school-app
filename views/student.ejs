<h1><%= title %></h1>
<table>
  <tr>
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Option</th>
  </tr>
  <%students.forEach((student)=>{%>
    <tr>
      <td><%=student.id%></td>
      <td><%=student.first_name%></td>
      <td><%=student.last_name%></td>
      <td><%=student.email%></td>
      <td>
        <a href="/student/edit/<%=student.id%>">Edit</a>
        <a href="/student/delete/<%=student.id%>">Delete</a>
      </td>
    </tr>
  <%})%>
</table>
<a href="/student/add">Add New Student</a>
