<h1><%= title %></h1>
<table>
  <tr>
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
  <%teachers.forEach((teacher)=>{%>
    <tr>
      <td><%=teacher.id%></td>
      <td><%=teacher.first_name%></td>
      <td><%=teacher.last_name%></td>
      <td><%=teacher.email%></td>
    </tr>
  <%})%>
</table>
